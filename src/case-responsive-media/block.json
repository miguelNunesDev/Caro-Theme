{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ink-components/case-responsive-media",
	"version": "0.1.0",
	"title": "Case responsive media",
	"category": "design",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": true
	},
	"attributes": {
		"desktop": {
			"type": "object",
			"default": {}
		},
		"mobile": {
			"type": "object",
			"default": {}
		},
		"height": {
			"type": "object",
			"default": {
				"lg": 0,
				"sm": 0
			}
		},
		"title": {
			"type": "string",
			"default": ""
		},
		"anchor": {
			"type": "string",
            "default": "#"
		}
	},
	"textdomain": "case-responsive-media",
	"editorScript": "file:./index.js",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
